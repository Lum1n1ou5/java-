import java.sql.Time;

public class Girl {

    private int name;
    private int age;
    private int height;
    private int weight;

    public void setAge(int age) {
        this.age = age;
    }

    public int getAge() {
        return age;
    }

    public void setName(int name) {
        this.name = name;
    }

    public int getName() {
        return name;
    }

    public void setHeight(int height) {
        this.height = height;
    }

    public int getHeight() {
        return height;
    }

    public void setWeight(int weight) {
        this.weight = weight;
    }

    public int getWeight() {
        return weight;
    }

    public void printSomething() {
        System.out.println("你好");
    }
}
